{
  "name": "andrebian/doctrine-entity-traits",
  "description": "A set of basic traits for Doctrine Entities",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andre Cardoso",
      "email": "andrecardosodev@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.1",
    "zendframework/zend-hydrator": "@stable",
    "doctrine/orm": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable"
  },
  "autoload": {
    "psr-4": {
      "Andrebian\\Doctrine\\EntityTraits\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\Andrebian\\Doctrine\\EntityTraits\\": "test/"
    }
  }
}
